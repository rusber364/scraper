{
  "name": "scraper",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "types": "tsc --noEmit",
    "bundle": "esbuild src/main.ts --bundle --platform=node --outfile=build/scraper.cjs",
    "blob": "node --experimental-sea-config sea-config.json",
    "create_executable": "node -e \"require('fs').copyFileSync(process.execPath, 'build/scraper.exe')\"",
    "inject": "postject build/scraper.exe NODE_SEA_BLOB build/scraper.blob --sentinel-fuse NODE_SEA_FUSE_fce680ab2cc467b6e072b8b5df1996b2",
    "build": "npm run bundle & npm run blob & npm run create_executable & npm run inject",
    "lint": "eslint src --ignore-path .gitignore --ext ts,tsx --color --fix --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write ./src",
    "converter": "tsx src/modules/converter/main.ts"
  },
  "dependencies": {
    "@hono/node-server": "^1.8.0",
    "axios": "^1.6.7",
    "cheerio": "1.0.0-rc.12",
    "hono": "^4.0.3",
    "js-md5": "^0.8.3"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "^20.11.10",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "esbuild": "^0.20.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "postject": "1.0.0-alpha.6",
    "prettier": "^3.2.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
